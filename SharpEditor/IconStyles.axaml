				<Path Data="{StaticResource FilledNewConfigIconGeometry}"
					  Fill="{StaticResource ThemeForegroundLowBrush}"
					  Stretch="None"
					  Width="{TemplateBinding Width}"
					  Height="{TemplateBinding Height}"/>
			</ControlTemplate>
		</Setter>
